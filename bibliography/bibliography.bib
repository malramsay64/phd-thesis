  issn = {1744-6848},
  journaltitle = {Soft Matter},
  langid = {english},
  number = {39},
  pages = {7795-7808},
  title = {Design Rules for Glass Formation from Model Molecules Designed by a Neural-Network-Biased Genetic Algorithm},
  url = {http://pubs.rsc.org/en/content/articlelanding/2019/sm/c9sm01486a},
  urldate = {2019-11-08},
