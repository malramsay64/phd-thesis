  issn = {0021-9606},
  journaltitle = {J. Chem. Phys.},
  number = {2},
  pages = {024505},
  shorttitle = {Dynamics of a Molecular Glass Former},
  title = {Dynamics of a Molecular Glass Former: {{Energy}} Landscapes for Diffusion in Ortho-Terphenyl},
