  file = {/home/malcolm/Zotero/storage/EFDYRY57/Schütt et al_2014_How to represent crystal structures for machine learning.pdf;/home/malcolm/Zotero/storage/P7ZFIZID/Schütt et al_2014_How to represent crystal structures for machine learning.pdf;/home/malcolm/Zotero/storage/YVBJKVQR/PhysRevB.89.html}
