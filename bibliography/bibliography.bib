  issn = {1525-2027},
  journaltitle = {Geochem. Geophys. Geosystems},
  keywords = {bootstrap,isochron fitting,maximum likelihood,nonlinear,probability,regression,uncertainty},
  langid = {english},
  number = {7},
  pages = {1-17},
  title = {Application of Maximum Likelihood and Bootstrap Methods to Nonlinear Curve-Fit Problems in Geochemistry},
  url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2001GC000253},
