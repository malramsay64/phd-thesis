  issn = {00219606},
  journaltitle = {J. Chem. Phys.},
  number = {12},
  pages = {6302-6313},
  title = {Model for the Translation-Rotation Coupling of Molecular Motion in Water},
  url = {http://scitation.aip.org/content/aip/journal/jcp/119/12/10.1063/1.1601599},
  urldate = {2016-09-19},
