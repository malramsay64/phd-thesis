  date = {2009},
  pages = {1168-1184},
  keywords = {force network ensemble,granular materials,monte carlo simulation},
  author = {van Eerd, A.R.T. and Tighe, B.P. and Vlugt, T.J.H.},
  options = {useprefix=true}
