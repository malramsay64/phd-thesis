  issn = {1932-6203},
  journaltitle = {PLOS ONE},
  keywords = {Breast cancer,Gene amplification,Gene expression,Messenger RNA,MicroRNAs,Programming languages,Relational databases,Reproducibility},
  langid = {english},
  number = {10},
  pages = {e0164023},
  title = {Language-{{Agnostic Reproducible Data Analysis Using Literate Programming}}},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0164023},
