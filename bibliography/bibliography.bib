  editor = {Xia, Feng},
  file = {/home/malcolm/Zotero/storage/QDJDB875/Rougier et al_2017_Sustainable computational science.pdf},
  issn = {2376-5992},
  journaltitle = {PeerJ Computer Science},
  keywords = {Computational science,GitHub,Open peer-review,Open science,Publication,Replicable,Reproducible,Sustainable},
