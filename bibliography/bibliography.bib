  author = {Salcedo, Elvin D. and Nguyen, Hong T. and Hoy, Robert S.},
  file = {/home/malcolm/Zotero/storage/ULF5NGK4/Salcedo et al_2019_Factors influencing thermal solidification of bent-core trimers.pdf;/home/malcolm/Zotero/storage/MIGKYPYC/1.html}
