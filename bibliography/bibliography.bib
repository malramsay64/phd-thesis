  file = {/home/malcolm/Zotero/storage/TMK7AS62/Sun et al_2018_Overcoming the Time Limitation in Molecular Dynamics Simulation of Crystal.pdf;/home/malcolm/Zotero/storage/CAF66KEJ/PhysRevLett.120.html}
}

@article{Yagasaki2016,
