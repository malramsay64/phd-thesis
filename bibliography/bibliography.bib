  date = {2011-05},
  pages = {1120-1129},
  keywords = {Dynamics of supercooled liquids,GPU computing,GRAPHICS PROCESSING UNITS,LENNARD-JONES MIXTURE,MODE-COUPLING THEORY,Molecular dynamics simulations,MOLECULAR-DYNAMICS},
  author = {Colberg, Peter H. and Höfling, Felix},
