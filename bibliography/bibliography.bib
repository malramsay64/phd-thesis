  pages = {041205},
  author = {Mossa, S. and La Nave, E. and Stanley, H. E. and Donati, C. and Sciortino, F. and Tartaglia, P.},
  file = {/home/malcolm/Zotero/storage/6MD3EBUQ/Mossa et al_2002_Dynamics and configurational entropy in the Lewis-Wahnstr-om model for.pdf;/home/malcolm/Zotero/storage/IHL9LJ7I/Mossa et al_2002_Dynamics and configurational entropy in the Lewis-Wahnstr-om model for.pdf;/home/malcolm/Zotero/storage/32HUSR3C/PhysRevE.65.html;/home/malcolm/Zotero/storage/66AR9G6L/PhysRevE.65.html}
}

@article{Pedersen2011,
