  journaltitle = {Phys. Rev. E},
  keywords = {Condensed Matter - Soft Condensed Matter},
  number = {3},
  pages = {031705},
  shorttitle = {Complete Breakdown of the {{Debye}} Model of Rotational Relaxation near the Isotropic-Nematic Phase Boundary},
  title = {Complete Breakdown of the {{Debye}} Model of Rotational Relaxation near the Isotropic-Nematic Phase Boundary: {{Effects}} of Intermolecular Correlations in Orientational Dynamics},
