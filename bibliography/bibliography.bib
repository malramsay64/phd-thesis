  file = {/home/malcolm/Zotero/storage/B8IEVWYK/McDonagh et al. - 2019 - Machine-Learned Fragment-Based Energies for Crysta.pdf;/home/malcolm/Zotero/storage/KV3Z6X86/McDonagh et al_2019_Machine-Learned Fragment-Based Energies for Crystal Structure Prediction.pdf;/home/malcolm/Zotero/storage/EKTL364M/acs.jctc.html}
}

