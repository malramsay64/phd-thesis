  file = {/home/malcolm/Zotero/storage/EZPWXMKM/Nguyen_Molinero_2015_Identification of Clathrate Hydrates, Hexagonal Ice, Cubic Ice, and Liquid.pdf;/home/malcolm/Zotero/storage/R4NUX3RT/jp510289t.html},
  issn = {1520-6106},
  journaltitle = {J. Phys. Chem. B},
  keywords = {Detection},
  number = {29},
  pages = {9369-9376},
  shorttitle = {Identification of {{Clathrate Hydrates}}, {{Hexagonal Ice}}, {{Cubic Ice}}, and {{Liquid Water}} in {{Simulations}}},
  title = {Identification of {{Clathrate Hydrates}}, {{Hexagonal Ice}}, {{Cubic Ice}}, and {{Liquid Water}} in {{Simulations}}: The {{CHILL}}+ {{Algorithm}}},
