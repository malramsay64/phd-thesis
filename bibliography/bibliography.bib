  pages = {379-473},
  keywords = {bond-orientational   order,bulk amorphous-alloys,free-volume model,molecular-dynamics simulations,real quasi-crystals,reverse monte-carlo,shear transformation zones,short-range order,supercooled liquid region,x-ray-diffraction},
  author = {Cheng, Y. Q. and Ma, E.},
  file = {/home/malcolm/Zotero/storage/2CJLZBU5/Atomic-level structure and structure–property relationship in metallic glasses.pdf;/home/malcolm/Zotero/storage/C3WHT3AL/Cheng_Ma_2011_Atomic-level structure and structure–property relationship in metallic glasses.pdf;/home/malcolm/Zotero/storage/L2TXM78Q/S0079642510000691.html}
