@article{Jiang1995,
  title = {Vapour-Liquid Equilibria in Two-Dimensional {{Lennard}}-{{Jones}} Fluids: Unperturbed and Substrate-Mediated Films},
  volume = {86},
  issn = {0026-8976},
  url = {https://doi.org/10.1080/00268979500102221},
  doi = {10.1080/00268979500102221},
  shorttitle = {Vapour-Liquid Equilibria in Two-Dimensional {{Lennard}}-{{Jones}} Fluids},
  abstract = {The Gibbs ensemble Monte Carlo simulation technique has been applied to study vapour-liquid equilibrium for two-dimensional (2D) fluids of Lennard-Jones (LJ) molecules. Both unperturbed 2D films and films on a model graphite substrate are investigated; in the latter case the fluid parameters are chosen to model methane. Large systems are used, up to 8000 molecules in the case of the unperturbed 2D films. Finite-size effects are accounted for in the region near the critical point, and the critical temperatures and densities are estimated for both systems. For the unperturbed 2D LJ films we find the reduced critical temperature and density to be T*c = 0·498 ± 0·002, and ρ*c = 0·360 ± 0·005. The corresponding values for the monolayer film on graphite are T*c = 0·500 ± 0·002, and ρ*c = 0·368 ± 0·005. The influence of the graphite substrate on the coexistence curve is found to be small, as expected. Comparisons of the simulated coexistence curve with experimental data for methane on graphite show good agreement in the critical region, and for the coexisting liquid phase densities, but the simulated gas phase densities are smaller than experimental values.},
  number = {4},
  journaltitle = {Molecular Physics},
  urldate = {2019-10-25},
  date = {1995-11-01},
  pages = {599-612},
  author = {Jiang, Shaoyi and Gubbins, Keith E.},
  file = {/home/malcolm/Zotero/storage/SGF5JDIT/Jiang_Gubbins_1995_Vapour-liquid equilibria in two-dimensional Lennard-Jones fluids.pdf;/home/malcolm/Zotero/storage/NVRNEYG7/00268979500102221.html;/home/malcolm/Zotero/storage/VPZJSS63/00268979500102221.html}
}

