  file = {/home/malcolm/Zotero/storage/FFPHLSYF/Royall et al_2018_The race to the bottom.pdf}
}

@article{Vogiatzis2019,
  langid = {english},
  title = {Network {{Topology}} of the {{States Probed}} by a {{Glassy Polymer}} during {{Physical Aging}}},
  volume = {28},
