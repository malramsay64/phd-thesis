  issn = {0079-6425},
  journaltitle = {Progress in Materials Science},
  keywords = {-relaxation,Aging,And relationship of relaxation and property,Boson peak,Coupling,Dynamics,Flow behaviour,Flow unit,Glass transition,Heterogeneity,Mechanical behaviour,Metallic glass,Nearly constant loss,Properties,Rejuvenation,Relaxation,Structure},
