  journaltitle = {Phys. Rev. E},
  number = {1},
  pages = {012606},
  title = {Vibrational Properties of Two-Dimensional Dimer Packings near the Jamming Transition},
  url = {https://link.aps.org/doi/10.1103/PhysRevE.100.012606},
  urldate = {2019-11-08},
